{
  "name": "@restatedev/workflow",
  "version": "0.0.0",
  "description": "Vercel Workflow World implementation using Restate SDK",
  "author": "Restate Developers",
  "email": "code@restate.dev",
  "license": "MIT",
  "homepage": "https://github.com/restatedev/vercel-workflow#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/restatedev/vercel-workflow.git"
  },
  "bugs": {
    "url": "https://github.com/restatedev/vercel-workflow/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./internal/private": {
      "import": "./dist/internal/private.js",
      "require": "./dist/internal/private.cjs"
    },
    "./runtime": {
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "turbo run _test --filter={.}...",
    "_test": "vitest run",
    "build": "turbo run _build --filter={.}...",
    "_build": "tsc --noEmit && tsdown",
    "dev": "tsc --noEmit --watch",
    "clean": "rm -rf dist *.tsbuildinfo .turbo",
    "check:types": "turbo run _check:types --filter={.}...",
    "_check:types": "tsc --noEmit --project tsconfig.build.json",
    "lint": "eslint .",
    "check:exports": "turbo run _check:exports --filter={.}...",
    "_check:exports": "attw --pack .",
    "check:api": "turbo run _check:api --filter={.}...",
    "_check:api": "api-extractor run --local",
    "prepublishOnly": "pnpm -w verify"
  },
  "peerDependencies": {
    "@workflow/world": "catalog:",
    "@workflow/core": "catalog:",
    "@workflow/errors": "catalog:",
    "@restatedev/restate-sdk": "catalog:",
    "@restatedev/restate-sdk-zod": "catalog:",
    "@restatedev/pubsub-client": "catalog:",
    "@vercel/queue": "catalog:",
    "workflow": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@workflow/world": "catalog:",
    "@workflow/errors": "catalog:",
    "@restatedev/restate-sdk-clients": "catalog:",
    "@restatedev/restate-sdk-zod": "catalog:",
    "@restatedev/pubsub-client": "catalog:",
    "@vercel/queue": "catalog:",
    "workflow": "catalog:",
    "zod": "catalog:"
  }
}
